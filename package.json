{
  "name": "html-to-text",
  "version": "5.1.1",
  "description": "Advanced html to plain text converter",
  "main": "dist/html-to-text.js",
  "scripts": {
    "build": "rm -Rf dist && tsc",
    "test": "jest",
    "prepublish": "npm test && npm build",
    "prettier:check": "prettier --ignore-path .eslintignore --check '**/*.{js,jsx,ts,tsx,md,json}'",
    "lint": "npm run prettier:check && eslint --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "eslint.js --ext .js,.jsx,.ts,.tsx . --fix; prettier --ignore-path .eslintignore --write '**/*.{js,jsx,ts,tsx,md,json}'"
  },
  "author": {
    "name": "Malte Legenhausen",
    "email": "legenhausen@werk85.de"
  },
  "homepage": "https://github.com/werk85/node-html-to-text",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/werk85/node-html-to-text.git"
  },
  "bugs": {
    "url": "https://github.com/werk85/node-html-to-text/issues"
  },
  "dependencies": {
    "@types/he": "^1.1.1",
    "@types/lodash": "^4.14.149",
    "@types/minimist": "^1.2.0",
    "@types/node": "^13.9.8",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-simple-import-sort": "^5.0.2",
    "he": "^1.2.0",
    "htmlparser2": "^4.1.0",
    "lodash": "^4.17.15",
    "minimist": "^1.2.5"
  },
  "keywords": [
    "html",
    "node",
    "text",
    "mail",
    "plain",
    "converter"
  ],
  "engines": {
    "node": ">= 10.0.0"
  },
  "bin": {
    "html-to-text": "./bin/cli.js"
  },
  "devDependencies": {
    "@types/jest": "^25.1.4",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-jest": "^23.8.2",
    "jest": "^25.2.4",
    "prettier": "^2.0.2",
    "ts-jest": "^25.3.0",
    "ts-node": "^8.8.1",
    "typescript": "^3.8.3"
  }
}
